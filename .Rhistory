density_portion <- function(){
# Create data
my_variable=rnorm(2000, 0 , 10)
# Calculate histogram, but do not draw it
my_hist=hist(my_variable , breaks=40  , plot=F)
# Color vector
my_color= ifelse(my_hist$breaks < -10, rgb(0.2,0.8,0.5,0.5) , ifelse (my_hist$breaks >=10, "purple", rgb(0.2,0.2,0.2,0.2) ))
# Final plot
plot(my_hist, col=my_color , border=F , main="" , xlab="value of the variable", xlim=c(-40,40) )
box()
}
density_portion()
qnorm(0.23, 0, 10)
norm_dens_portion <- function(ave = 0, var = 10, probability = 0.2){
# Create data
my_variable=rnorm(2000, ave, var)
# Calculate histogram, but do not draw it
my_hist=hist(my_variable , breaks=40  , plot=F)
# Color vector
my_color= ifelse(my_hist$breaks < -qnorm(probability, ave, var),
rgb(0.2,0.8,0.5,0.5) ,
ifelse (my_hist$breaks >=qnorm(probability, ave, var),
"purple", rgb(0.2,0.2,0.2,0.2) ))
# Final plot
plot(my_hist, col=my_color , border=F , main="" ,
xlab="value of the variable", xlim=c(-40,40) )
box()
}
norm_dens_portion(3, 12, 0.4)
